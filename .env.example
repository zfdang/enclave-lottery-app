```bash
# Environment variables template
# Copy this file to .env and fill in real values for local development

# Blockchain configuration (use ETHEREUM_* variables where possible)
ETHEREUM_RPC_URL=http://localhost:8545
CHAIN_ID=31337
CONTRACT_ADDRESS=
PRIVATE_KEY=

# Server configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# Enclave configuration
ENCLAVE_VSOCK_PORT=5000
ENCLAVE_ATTESTATION_ENABLED=true

# Lottery configuration
LOTTERY_DRAW_INTERVAL_MINUTES=10
LOTTERY_MINIMUM_INTERVAL_MINUTES=3
LOTTERY_BETTING_CUTOFF_MINUTES=1
LOTTERY_SINGLE_BET_AMOUNT=0.01
LOTTERY_MAX_BETS_PER_USER=100

# Application behavior
DEBUG=false
LOG_LEVEL=INFO

# Frontend configuration
REACT_APP_WS_URL=ws://localhost:8080
REACT_APP_API_URL=http://localhost:8080
``` 